/*
 * To implement additional custom commands for elfinder, drop command definitions inside your Rails application within the
 * app/assets/javascripts/elfinder/commands directory.
 */
<%
  dir = Rails.root.join('app','assets','javascripts','elfinder','commands','local')
  if File.directory?(dir)
    Dir["#{dir}/*.js"].each { |js| %><%= evaluate(js) %><% }
  end
%>
